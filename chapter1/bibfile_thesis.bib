@article {Manchester542,
	author = {Manchester, R. N.},
	title = {Observational Properties of Pulsars},
	volume = {304},
	number = {5670},
	pages = {542--546},
	year = {2004},
	doi = {10.1126/science.1097649},
	publisher = {American Association for the Advancement of Science},
	abstract = {Pulsars are remarkable clocklike celestial sources that are believed to be rotating neutron stars formed in supernova explosions. They are valuable tools for investigations into topics such as neutron star interiors, globular cluster dynamics, the structure of the interstellar medium, and gravitational physics. Searches at radio and x-ray wavelengths over the past 5 years have resulted in a large increase in the number of known pulsars and the discovery of new populations of pulsars, posing challenges to theories of binary and stellar evolution. Recent images at radio, optical, and x-ray wavelengths have revealed structures resulting from the interaction of pulsar winds with the surrounding interstellar medium, giving new insights into the physics of pulsars.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/304/5670/542},
	eprint = {http://science.sciencemag.org/content/304/5670/542.full.pdf},
	journal = {Science}
}

